/* pages/home/home.wxss */
.slide-image{  
    width: 100%;  
} 
.tab {
  background-color: #fff;
  font-size: 28rpx;
}
.search-wrap {
    display: flex;
    align-items: center;
    width: 100%;
    height: 90rpx;
    color: #fff;
    background: #f6f6f6;
}
.search {
  flex: 1;
  margin-left: 24rpx;
}
.fee {
  height: auto;
  background-color: #fff;
  display: flex;
  border-bottom: 1px solid #ededed;
} 
.iconPart {
  text-align: center;
  padding:30rpx 0;
  font-size: 28rpx;
}
.iconPart image {
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}
.filter{
  text-align: center;
  height:60rpx;
  line-height:60rpx;
}
.picker image {
  width: 30rpx;
  height: 26rpx;
  margin-left: 10rpx;
}
.scroll-view-head {
  margin: 10rpx 0;
  background-color: #ededed;
}
.scroll-view-item {
  height: auto;
  line-height: 50rpx;
  text-align: center;
}