/* pages/home/home.wxss */
.slide-image{  
    width: 100%;  
} 
.tab {
  background-color: #fff;
  font-size: 28rpx;
  height: 42rpx;
  line-height: 42rpx;
}
.search-wrap {
    display: flex;
    align-items: center;
    width: 100%;
    height: 90rpx;
    color: #fff;
    background: #f6f6f6;
}
.search {
  flex: 1;
  margin-left: 24rpx;
}
.fee {
  height: auto;
  background-color: #fff;
  display: flex;
  border-bottom: 1px solid #ededed;
} 
.iconPart {
  text-align: center;
  padding:30rpx 0;
  font-size: 28rpx;
}
.iconPart image {
  width: 70rpx;
  height: 70rpx;
  margin: 0 auto;
  margin-bottom: 12rpx;
}
.filter{
  text-align: center;
  height:68rpx;
  line-height:68rpx;
}
.picker image {
  width: 30rpx;
  height: 26rpx;
  margin-left: 10rpx;
}
.scroll-view-head {
  margin: 10rpx 0;
  background-color: #F4F4F4;
  color: #666;
}
.scroll-view-item {
  height: auto;
  line-height: 68rpx;
  text-align: center;
}
.no-data {
  text-align: center;
  font-size: 24rpx;
  margin-top: 30rpx;
  color: #999;
}
.btn-add {
  margin: 0 30rpx;
  height: 88rpx;
  line-height: 88rpx;
  font-size: 28rpx;
  position: fixed;
  bottom:20rpx;
  left: 0;
  right: 0;
}